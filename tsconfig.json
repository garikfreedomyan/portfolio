{
  "compilerOptions": {
    "target": "es5",
    "lib": ["dom", "dom.iterable", "esnext"],
    "allowJs": true,
    "skipLibCheck": true,
    "esModuleInterop": true,
    "allowSyntheticDefaultImports": true,
    "strict": true,
    "forceConsistentCasingInFileNames": true,
    "noFallthroughCasesInSwitch": true,
    "module": "esnext",
    "moduleResolution": "node",
    "resolveJsonModule": true,
    "isolatedModules": true,
    "noEmit": true,
    "jsx": "react",
    "rootDir": ".",
    "baseUrl": "src",
    "paths": {
      "components": ["components"],
      "components/*": ["components/*"],
      "constants": ["constants"],
      "constants/*": ["constants/*"],
      "contexts": ["contexts"],
      "contexts/*": ["contexts/*"],
      "hooks": ["hooks"],
      "hooks/*": ["hooks/*"],
      "img/*": ["img/*"],
      "img": ["img"],
      "modules": ["modules"],
      "modules/*": ["modules/*"],
      "pages": ["pages"],
      "pages/*": ["pages/*"],
      "services": ["services"],
      "services/*": ["services/*"],
      "store": ["store"],
      "store/*": ["store/*"],
      "types": ["types"],
      "types/*": ["types/*"],
      "routes": ["routes"],
      "routes/*": ["routes/*"],
      "utils": ["utils"],
      "utils/*": ["utils/*"]
    }
  },
  "include": ["src"]
}
